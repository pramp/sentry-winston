{
  "name": "sentry-winston",
  "version": "1.0.1",
  "description": "A winston transport that sends stuff to sentry.",
  "main": "./lib/winston-sentry.js",
  "scripts": {
    "prepare": "node_modules/babel-cli/bin/babel.js src --out-dir lib"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/pramp/sentry-winston.git"
  },
  "keywords": [
    "sentry",
    "winston"
  ],
  "author": "Omer Gelbard",
  "license": "Apache-2.0",
  "homepage": "https://gitlab.com/pramp/sentry-winston#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0"
  },
  "peerDependencies": {
    "winston": ">= 2.4.2"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "dependencies": {
    "core-util-is": "latest"
  }
